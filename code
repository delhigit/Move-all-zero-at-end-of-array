#include<iostream>
using namespace std;

int main()
 {
	int t;
	cin >> t;
	
	while(t--)
	{
	    int num, zero = 0;
	    cin >> num;
	    
	    int arr[num];
	    
	    for(int i = 0; i < num; i++)
	    {
	        cin >> arr[i];
	        
	        if(arr[i] != 0)
	        arr[zero++] = arr[i];
	    }
	    
	    for(int i = zero; i < num; i++)
	    arr[i] = 0;
	    
	    for(int i = 0; i < num; i++)
	    cout << arr[i] << " " ;
	    
	    cout << endl;
	}
	
	return 0;
}
